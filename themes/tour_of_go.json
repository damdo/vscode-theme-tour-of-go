{
    "name": "TourOfGo",
    "type": "dark",
    "colors": {
        "editor.background": "#FFFFD8",
        "editor.lineHighlight":"#D7D3F0",
        "editor.selectionHighlight":"#D7D3F0",
        "editor.rangeHighlight":"#ff0000",
        "editor.selectionBackground":"#D7D3F0",
        "editor.inactiveSelection":"#00F",
        "editor.findMatchHighlight":"#00F",
        "editor.referenceHighlight":"#00F",
        "editor.currentFindMatchHighlight":"#00F",
        "editor.findRangeHighlight":"#00F",
        "editor.hoverHighlight":"#00F",
        "editorLineNumber.foreground":"#999"
    },

    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#A50"
            }
        },
        {
            "name": "Keywords, Punctuation",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "punctuation.definition.parameters",
                "punctuation.section.parens.begin",
                "variable.parameter",
                "variable.other.property",
                "support.variable.property.dom",
                "variable.other.object.property",
                "punctuation.separator",
                "punctuation.accessor",
                "punctuation.definition.array"
            ],
            "settings": {
                "foreground": "#708"
            }
        },
        {
            "name": "Objects",
            "scope": [
                "variable.other.object",
                "support.class.console",
                "support.variable.dom",
                "support.constant",
                "entity.name.type.module",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Booleans",
            "scope": [
                "constant.language.boolean.true",
                "constant.language.boolean.false",
                "constant.language.null"
            ],
            "settings": {
                "foreground": "#219"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#219"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#A11"
            }
        },
        {
            "name": "Quotes",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#A11",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Method definitions",
            "scope": [
                "entity.name.function",
                "support.function.dom",
                "support.function"
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.language",
                "variable.other.member",
                "variable.other.readwrite",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#000000"
            }
        }
    ]
}
